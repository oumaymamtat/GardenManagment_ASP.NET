
@{
    ViewBag.Title = "SuccessPage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<div class="page-content bg-white">
    <!-- inner page banner -->
    <div class="dlab-bnr-inr overlay-black-middle bg-pt" style="background-image:url(/Content/images/banner/bnr4.jpg);">
        <div class="container">
            <div class="dlab-bnr-inr-entry">

                <!-- Breadcrumb row -->
                <div class="breadcrumb-row">
                    <ul class="list-inline">
                        <li><a href="@Url.Action("Index","Home")">Home</a></li>
                        <li>Forget Password</li>
                        <li>Verify Code</li>
                    </ul>
                </div>
                <!-- Breadcrumb row END -->
            </div>
        </div>
    </div>
    <!-- inner page banner END -->
    <!-- Breadcrumb row END -->
    <div class="bg-white alert-box-area">
        <div class="section-full bg-white content-inner">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="sort-title clearfix text-center">
                            <h4>Success </h4>
                        </div>
                        <!-- Alert box -->
                        <div class="section-content box-sort-in m-b30">
                            <div class="alert alert-success"> <strong>Password </strong> is successfully changed !</div>
                       <a href="@Url.Action("Login","User")" class="btn btn-link">Login Again</a>
                         
                        </div>
                    </div>
                    <!-- Alert box END -->
                </div>
            </div>

        </div>
    </div>
</div>

